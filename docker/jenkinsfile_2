def COLOR

pipeline {
    agent{
    	label 'agent_2'
    }
    
    stages {
        stage('Initialize') {
            steps {
                //cd 'agent\\scripts\\'
                sh 'dir'
                COLOR = "green"
                echo "color is $COLOR"

                sh """COLOR=${COLOR}
                echo $COLOR"""

            }
        }
    }
}
