//Jenkins Node's scripts folder
def smbt_scripts = '/home/jenkins/agent/scripts/'

pipeline {
    agent{
    	label 'Jenkins_Node'
    }
    
    stages {
        stage('Initialize') {
            steps {
                bat 'wmic computersystem get name'
                cd smbt_scripts
                call 'Hello.bat'
            }
        }
    }
}
